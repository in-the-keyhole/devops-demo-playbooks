apiVersion: v1
kind: Service
metadata:
  labels:
    app: {{ app.name }}
  name: {{ app.name }}
spec:
  ports:
  - name: "{{ app.port }}-tcp"
    port: {{ app.port }}
    protocol: TCP
    targetPort: {{ app.port }}
  selector:
    deploymentconfig: {{ app.name }}
  sessionAffinity: None
  type: ClusterIP
status:
  loadBalancer: {}
